<p>
  <strong>Body:</strong>
  <% @entry.chord_body.each do |section| %>
    <div>
      <p><%= section[:title] %></p>
      <div>
        <% section[:measures].each do |measure|%>
          <span>
            <% measure.each do |chord| %>
              <span><%= chord %></span>
            <% end %>
          </span>
        <% end %>
      </div>
    </div>
  <% end %>
</p>
